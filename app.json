{
  "expo": {
    "name": "CalLog",
    "slug": "CalcuLogin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/calculator.png",
    "scheme": "calculogin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/calculator.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tichah.CalcuLogin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/calculator.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/calculator.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "de3a4f02-f4ad-4508-8e69-466188777d17"
      }
    }
  }
}
